{
  "name": "vue3-base-config",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve:qa": "vue-cli-service serve --mode preview",
    "serve:pro": "vue-cli-service serve --mode production",
    "build:dev": "vue-cli-service build --mode development",
    "build:qa": "vue-cli-service build --mode preview",
    "build": "vue-cli-service build --mode production",
    "report":"cross-env use_analyzer=true vue-cli-service build",
    "inspect": "vue-cli-service inspect --development",
    "lint": "vue-cli-service lint",
    "checklint": "eslint --ext .js,.vue --ignore-path .gitignore . ",
    "fix": "eslint --fix --ext .js,.vue --ignore-path .gitignore . ",
    "format": "npx prettier --write .",
    "prepare": "husky install",
    "commit": "git-cz"
  },
  "dependencies": {
    "ant-design-vue": "^2.2.8",
    "core-js": "^3.6.5",
    "lint-staged": "^12.1.7",
    "vue": "^3.2.0",
    "vue-class-component": "^8.0.0-0",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^5.9.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.0",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "husky": "^6.0.0",
    "less": "^2.7.0",
    "less-loader": "^5.0.0",
    "prettier": "^2.5.1",
    "terser-webpack-plugin": "^2.3.8",
    "typescript": "~4.1.5",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "@vue/typescript/recommended",
      "@vue/prettier",
      "@vue/prettier/@typescript-eslint"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "lint-staged": {
    "*.{vue,js,ts}": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "description": "vue3+vuex+antdv 搭建的项目基础架构,帮助快速搭建vue3代码架构,可以按需根据项目需求在此基础上进行改进或者直接使用",
  "keywords": [
    "vue3",
    "typescript",
    "antdv",
    "ant-design-vue",
    "vuex",
    "srj"
  ]
}
